{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "lib": ["ES2022"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../dist/workers-cjs",
    "rootDir": ".",
    "declaration": true,
    "sourceMap": true
  },
  "include": ["workflows.ts", "types.ts"],
  "exclude": ["node_modules"]
}
